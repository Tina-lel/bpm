# Package info (Example Package v1)
DESCRIPTION="Example package for BPM"

SORT="main"

DEPENDENCIES=""
DOWNLOAD="false"
COMPILE="true"

INSTALLED_FILES=(/usr/local/bin/bpm_example)

COMPILE_SRC() {
    gcc -o $PKG_DIR/build/bpm_example -O2 $PKG_DIR/src/main.c
}

INSTALL_PKG() {
    mv $PKG_DIR/build/bpm_example /usr/local/bin/
}
